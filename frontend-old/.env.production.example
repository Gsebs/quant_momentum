# API Endpoints (Replace with your production URLs)
NEXT_PUBLIC_API_BASE_URL=https://api.your-domain.com
NEXT_PUBLIC_WS_URL=wss://api.your-domain.com/ws
NEXT_PUBLIC_ML_MODEL_URL=https://api.your-domain.com/ml

# Exchange WebSocket URLs
NEXT_PUBLIC_COINBASE_WS_URL=wss://ws-feed.pro.coinbase.com
NEXT_PUBLIC_BINANCE_WS_URL=wss://stream.binance.com:9443/ws

# Trading Parameters
NEXT_PUBLIC_MIN_PROFIT_THRESHOLD=0.0005
NEXT_PUBLIC_MAX_LATENCY=50
NEXT_PUBLIC_TRADE_SIZE_BTC=0.01
NEXT_PUBLIC_TRADE_SIZE_ETH=0.1

# Environment
NEXT_PUBLIC_ENV=production

# Note: API keys and secrets should be set in Netlify's environment variables
# and not in this file or any committed files 